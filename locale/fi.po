#: ../src/lastore-session-helper/job.go:189
msgid "Deepin Store"
msgstr "Deepin Kauppa"

#: ../src/lastore-session-helper/job.go:190
msgid "System is updating, please shut down or reboot later."
msgstr "Järjestelmää päivitetään. Tämän jälkeen tulee laite sammuttaa tai käynnistää uudelleen."

#: ../src/lastore-session-helper/job.go:225
msgid "Reboot Now"
msgstr "Käynnistä uudelleen välittömästi"

#: ../src/lastore-session-helper/job.go:239
msgid "Reboot Later"
msgstr "Käynnistä uudelleen myöhemmin"

#: ../src/lastore-session-helper/job.go:248
msgid "Retry"
msgstr "Yritä uudestaan"

#: ../src/lastore-session-helper/job.go:256
msgid "Cancel"
msgstr "Peruutus"

#: ../src/lastore-session-helper/ntofiy.go:51
msgid "%q installed successfully."
msgstr "%q asennettu. "

#: ../src/lastore-session-helper/ntofiy.go:53
msgid "%q failed to install."
msgstr "Kohteen %q asennus epäonnistui."

#: ../src/lastore-session-helper/ntofiy.go:61
msgid "%q removed successfully."
msgstr "%q poistettu onnistuneesti. "

#: ../src/lastore-session-helper/ntofiy.go:63
msgid "%q failed to remove."
msgstr "Kohteen %q poistaminen ei onnistunut."

#: ../src/lastore-session-helper/ntofiy.go:70
msgid "%q failed to download."
msgstr "%q latautuminen epäonnistui."

#: ../src/lastore-session-helper/ntofiy.go:76
msgid ""
"In order to prevent automatic shutdown, please plug in for normal update."
msgstr "Kirjaudu sisään saadaksesi normaalipäivitykset ja estääksesi automaattisen sulkeutumisen."

#: ../src/lastore-session-helper/ntofiy.go:85
msgid "Some functions will take effect after rebooting."
msgstr "Jotkut toiminnot astuva voimaan vasta uudelleenkäynnistyksen jälkeen."

#: ../src/lastore-session-helper/ntofiy.go:85
#: ../src/lastore-session-helper/ntofiy.go:87
msgid "Updated successfully!"
msgstr "Päivitys onnistui!"

#: ../src/lastore-session-helper/ntofiy.go:90
msgid "Failed to update."
msgstr "Päivitys epäonnistui."

#: ../src/lastore-session-helper/ntofiy.go:106
#, c-format
msgid "%d software need to be updated."
msgstr "%d ohjelmistosta täytyy päivittää."

#: ../src/lastore-session-helper/ntofiy.go:108
msgid "Some patches need to be updated."
msgstr "Korjaustiedostoja täytyy päivittää."

#: ../src/lastore-session-helper/ntofiy.go:110
#, c-format
msgid "Some patches and %d software need to be updated."
msgstr "Korjaustiedostoja ja %d-ohjelmistoa täytyy päivittää."

#: ../src/lastore-session-helper/ntofiy.go:117
msgid "Update Now"
msgstr "Päivitä välittömästi"
